<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Yonghong BI</display-name>

    <servlet>
        <servlet-name>ViewerServlet</servlet-name>
        <servlet-class>g5.sv.PortalServlet</servlet-class>
        <init-param>
            <param-name>bi.home</param-name>
            <param-value>/Users/Charles/Documents/Yonghong/Yonghong/bihome</param-value>
        </init-param>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewerServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>

    <env-entry>
        <env-entry-name>bi.home</env-entry-name>
        <env-entry-value>/Users/Charles/Documents/Yonghong/Yonghong/bihome</env-entry-value>
        <env-entry-type>java.lang.String</env-entry-type>
    </env-entry>
</web-app>
